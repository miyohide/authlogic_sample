<h1>Login or <%= link_to 'Sign up', signup_path %></h1>
<%= form_for @user_session, as: :user_session, url: { action: 'create' } do |f| %>
  <%= f.label :email %> <%= f.text_field :email %><br />
  <%= f.label :password %> <%= f.password_field :password %><br />
  <%= f.check_box :remember_me %> remember me<br />
  <br />
  <%= f.submit "Login" %>
<% end %>

